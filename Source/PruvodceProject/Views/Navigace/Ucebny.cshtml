@model List<UcebnaModel>
@{
    ViewData["Title"] = "Seznam učeben";
}

<article> @* WIP *@
    @foreach (UcebnaModel ucebna in Model)
    {
        <section>
            <h3>@ucebna.idUcebny</h3>
            <a href="/Navigace/UcebnaDetail/@ucebna.Id">Zobrazit podrobné informace</a>
        </section>
    }
    @if (Model.Count == 0)
    {
        <h3>Žádné data</h3>
    }
</article>